  script(defer src='./assets/scripts/index.js')
  script(defer src='./assets/scripts/header.js')
  script(defer src='./assets/scripts/name=thankyoupage.js')
block content
  .thankyoupage
    .text-style-h-1.text-black Mesaj gönderildi
    .text-style-h-3.text-black Yöneticilerimizden cevap bekleyiniz
