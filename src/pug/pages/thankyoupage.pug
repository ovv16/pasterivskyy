    var title_page = 'name=thankyoupage'
    var id_page = 'id-page-name=thankyoupage'
    var class_page = 'class-page-name=thankyoupage'
    var breadcrumbsSubitem = [
      {
        label: 'About',
        link: pagesLink[0].about
      }
    ]
  
block meta
  title= title_page
  meta(name='description', content='')
